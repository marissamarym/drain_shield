<%= simple_form_for(@customer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :email %>
    <%= f.association :company %>
    <%= f.association :user %>
    <%= f.input :network_ssid %>
    <%= f.input :network_password %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
